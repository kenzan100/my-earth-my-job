<div id="canvas">
    <div id="title">
      <% @equipments.each do |equipment| %>
        <li>
          <%= equipment.name %>
        </li>
      <% end %>
    </div>
    <div>
      I have
      <span id="money"></span>
    </div>
</div>

<%= hidden_field_tag :starting_money, @current_val %>
<%= hidden_field_tag :hourly_rate, @current_rate %>

<br/>
Road to $1M
