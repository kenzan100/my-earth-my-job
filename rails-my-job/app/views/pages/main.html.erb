<div id="duration"></div>

<br/>

<p>Available Jobs</p>
<% @available_jobs.each do |job| %>
  <li>
    <%= link_to job.name, job_path(job) %>
  </li>
<% end %>

<br />

<%= link_to "Register new job", new_admin_equipment_path %>

